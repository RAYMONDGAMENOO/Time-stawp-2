            local target = tostring(game.Players.LocalPlayer.Character.BodyEffects.Grabbed.Value)
            wait(0.9)

            for i, Track in pairs(game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):GetPlayingAnimationTracks()) do
